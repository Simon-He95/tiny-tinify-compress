{
  "name": "tiny-tinify-compress",
  "version": "1.0.1",
  "packageManager": "pnpm@6.32.3",
  "description": "tiny-tinify-compress",
  "author": "Simon He",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git + git@github.com:Simon-He95/tiny-tinify-compress.git"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/tiny-tinify-compress/issues"
  },
  "keywords": [
    "tiny-tinify-compress"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "compress": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll --minify",
    "dev": "pkgroll --watch",
    "play": "pnpm run -C playground dev",
    "play:build": "pnpm run -C playground build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest --dir test -u",
    "ptest": "pnpm run -C playground test -u",
    "preview": "pnpm run -C playground preview",
    "test:e2e": "cypress open",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --tag --push && git push origin --tags -f && npm publish"
  },
  "dependencies": {
    "@vueuse/core": "^8.1.1",
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "figlet": "^1.5.2",
    "file-type": "^17.1.4",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "ora": "^6.1.2",
    "pkg-types": "^0.3.3",
    "tinify": "^1.6.1",
    "uuid": "^8.3.2",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.0",
    "@types/figlet": "^1.5.4",
    "@types/imagemin-gifsicle": "^7.0.1",
    "@types/node": "^17.0.38",
    "@vitejs/plugin-vue": "^3.0.1",
    "bumpp": "^7.1.1",
    "eslint": "^8.16.0",
    "eslint-plugin-n": "^15.2.1",
    "esno": "^0.16.3",
    "pkgroll": "^1.3.1",
    "typescript": "^4.7.2",
    "vite": "^3.0.2",
    "vitest": "^0.13.1"
  },
  "tinifyCompress": {
    "key": "DZkjgp25tVjhkLG8GXk0nF3tpNSkq7kX",
    "includes": [
      "./assets/**"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
